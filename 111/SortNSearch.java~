//Program Description: This program bubble sorting to sort
//                     an unsorted array of integers, and a 
//                     binary search which looks for a 
//                     certain number.
//***********************************************************

import java.util.*;
import java.io.*;

public class SortNSearch
{
  static Scanner infile = new Scanner(new FileReader("BubbleSortArrayInput.txt"));
  
  public static void main(String[] args) throws FileNotFoundException
  {
    int num = 100;
    int i, j, temp;
    int[] array = new int[num];
    
    for(i = 0; i < num; i++)
      array[i] = infile.nextInt();
      
    for(i = 0; i < num; i++)
      System.out.print(array[i]);
  }
}